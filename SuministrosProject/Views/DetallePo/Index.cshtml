@model IEnumerable<SuministrosProject.Models.DetallePo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
    @ViewData["PO"]
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdProductOrderNavigation.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdSuministroNavigation.Serie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cantidadPedido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CantidadRecibida)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pendiente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CantidadPendiente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observacion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdProductOrderNavigation.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSuministroNavigation.Serie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cantidadPedido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantidadRecibida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pendiente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantidadPendiente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdDetallePo }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdDetallePo }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdDetallePo })
            </td>
        </tr>
    }

</table>
