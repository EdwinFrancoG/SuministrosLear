
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Suministros Lear</title>
    <link rel="stylesheet" type="text/css" href="~/Content/assets/lib/stroke-7/style.css" />
    <link href="~/Content/assets/lib/stroke-7/style.css" rel="stylesheet" />
    <link href="~/Content/assets/lib/perfect-scrollbar/css/perfect-scrollbar.min.css" rel="stylesheet" />
    <link href="~/Content/assets/lib/datatables/datatables.net-bs4/css/dataTables.bootstrap4.css" rel="stylesheet" />
    @*<link rel="stylesheet" type="text/css" href="~/Content/lib/perfect-scrollbar/css/perfect-scrollbar.min.css" />*@
    <link href="~/Content/assets/lib/datatables/datatables.net-buttons-bs4/css/buttons.bootstrap4.css" rel="stylesheet" />
    <link href="~/Content/assets/lib/datatables/datatables.net-buttons-bs4/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/jquery.dataTables.min.css" rel="stylesheet" />



    <link rel="stylesheet" href="~/Content/assets/css/app.css" type="text/css" />
</head>
<body>
    <nav id="" class="navbar navbar-expand navbar-dark mai-top-header">
        <div class="container">
            <a href="#" class="navbar-brand"></a>
            @*nav navbar-nav float-lg-right mai-user-nav*@
            <ul class="nav navbar-nav float-lg-right mai-user-nav">
                <!--HOME-->
                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" onclick="PageRedirection('/Home/Index')" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name"> <span class="icon s7-home"></span>           HOME</span></a>
                </li>

                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" onclick="PageRedirection('/ProductOrder/Index')" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name"> <span class="icon s7-arc"></span>              PO</span></a>
                </li>

                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" onclick="PageRedirection('/Stocks/Index')" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name"> <span class="icon s7-ticket"></span>     STOCK</span></a>
                </li>

                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" onclick="GetCreateSalida()" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name"> <span class="icon s7-back-2"></span>     OUTPUT</span></a>
                </li>

                @* Reportes del sistema *@
                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name"> <span class="icon s7-notebook"></span>     REPORTS<span class="angle-down s7-angle-down"></span></span></a>
                    <div role="menu" class="dropdown-menu">
                        <a href="#" onclick="PageRedirection('/Salida/ReportByNumberPart')" class="dropdown-item"><span class="icon s7-news-paper"></span>   REPORT BY PART NUMBER</a>
                        <a href="#" onclick="PageRedirection('/Salida/viewReportSalida')" class="dropdown-item"><span class="icon s7-news-paper"> </span>REPORT BETWEEN DATES</a>
                        <a href="#" onclick="PageRedirection('/Salida/ReportByEquipment')" class="dropdown-item"> <span class="icon s7-news-paper"> </span>REPORT BY EQUIPMENT</a>
                    </div>
                </li>


                <!--Suministro-->
                <li class="dropdown nav-item" style="margin-right:5px;">
                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name">SUPPLY</span><span class="angle-down s7-angle-down"></span></a>
                    <div role="menu" class="dropdown-menu">
                        <a href="#" onclick="PageRedirection('/Categorias/Index')" class="dropdown-item"><span class="icon s7-ticket"></span>   SUPPLY TYPE</a>
                        <a href="#" onclick="PageRedirection('/Suministro/Index')" class="dropdown-item"><span class="icon s7-notebook"> </span>SUPPLY RECORD</a>
                        <a href="#" onclick="PageRedirection('/NumeroParte/Index')" class="dropdown-item"> <span class="icon s7-news-paper"> </span>PART NUMBER</a>
                        <a href="#" onclick="PageRedirection('/Localizacions/Index')" class="dropdown-item"> <span class="icon s7-map"> </span>LOCATIONS</a>
                        <a href="#" onclick="PageRedirection('/Entrada/Index')" class="dropdown-item"> <span class="icon s7-edit"> </span>INPUTS RECORD</a>
                        <a href="#" onclick="PageRedirection('/Salida/Index')" class="dropdown-item"> <span class="icon s7-back"> </span>OUTPUTS RECORD</a>
                    </div>
                </li>

                <!--Users-->
                <li class="dropdown nav-item">
                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name">USERS</span><span class="angle-down s7-angle-down"></span></a>
                    <div role="menu" class="dropdown-menu">
                        <a href="#" onclick="PageRedirection('/Usuario/Index')" class="dropdown-item"><span class="icon s7-ticket"></span>  USERS</a>
                        <a href="#" onclick="PageRedirection('/Perfils/Index')" class="dropdown-item"><span class="icon s7-notebook"> </span>PERFIL</a>
                    </div>
                </li>
            </ul>



            <!--User Menu-->
            <ul class="nav navbar-nav float-lg-right mai-user-nav">
                <li class="dropdown nav-item">
                    <a href="#" onclick="LogOut()" data-toggle="dropdown" role="button" aria-expanded="false" class="dropdown-toggle nav-link"><span class="user-name" style="color:red;">LOG OUT</span></a>
                </li>
            </ul>
        </div>
    </nav>
    <br />
    <br />

    <div class="container body-content" style="background-color:white;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - all rights reserved - Lear Corporation. </p>
        </footer>
    </div>


    @* Para mostrar data del get de create *@
    <div class="modal fade" id="NewModalS" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content" style="max-width: 750px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div id="NewModalBodyS" class="modal-body">
                </div>
            </div>
        </div>
    </div>

    @* Modal para una salida correcta *@
    <div id="idAlertPositiveSalida" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="close"><span class="s7-close"></span></button>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="text-primary"><span class="modal-main-icon s7-check"></span></div>
                        <h3 class="mb-4">La salida del suministro se hizo correctamente</h3>
                        <p id="BodyPositiveSalida"></p>
                        <div class="mt-6">
                            <button type="button" onclick="redireccionarStock()" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">Stock</button>
                            <button id="BotonLimpiar" type="button" onclick="" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">Nueva Salida</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*Modal error*@
    <div id="idAlertNegativeS" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="close"><span class="s7-close"></span></button>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="text-danger"><span class="modal-main-icon s7-close-circle"></span></div>
                        <h3 class="mb-4">Danger!</h3>
                        <p id="BodyNegativeS"></p>
                        <div class="mt-6">
                            <button type="button" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">Acept</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="NewModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content" style="max-width: 750px;">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div id="NewModalBody" class="modal-body">
                </div>
            </div>
        </div>
    </div>

    @*Modal ok*@
    <div id="idAlertPositiveS" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="close"><span class="s7-close"></span></button>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="text-primary"><span class="modal-main-icon s7-check"></span></div>
                        <h3 class="mb-4">Awesome!</h3>
                        <p id="StockBodyPositive"></p>
                        <div class="mt-6">
                            <button type="button" onclick="redireccionarIndex()" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">Acept</button>
                            <button type="button" onclick="redireccionarStock()" data-dismiss="modal" class="btn btn-sm btn-space btn-primary">Stock</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*Modal actualizacion de usuario correcta*@
    <div id="idAlertPositiveU" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="close"><span class="s7-close"></span></button>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="text-primary"><span class="modal-main-icon s7-check"></span></div>
                        <h3 class="mb-4">Awesome!</h3>
                        <p id="BodyPositiveU"></p>
                        <div class="mt-6">
                            <button type="button" onclick="RedireccionarUser()" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">OK</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*Modal error*@
    <div id="idAlertNegative" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" data-dismiss="modal" aria-hidden="true" class="close"><span class="s7-close"></span></button>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="text-danger"><span class="modal-main-icon s7-close-circle"></span></div>
                        <h3 class="mb-4">Danger!</h3>
                        <p id="BodyNegative"></p>
                        <div class="mt-6">
                            <button type="button" data-dismiss="modal" class="btn btn-sm btn-space btn-secondary">Acept</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", required: false)



    <script src="~/Content/assets/lib/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/bootstrap/dist/js/bootstrap.bundle.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/jquery.flot.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/jquery.flot.pie.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/jquery.flot.time.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/jquery.flot.resize.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/plugins/jquery.flot.orderBars.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/plugins/curvedLines.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/jquery-flot/plugins/jquery.flot.tooltip.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/countup/countUp.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/js/app-dashboard.js" type="text/javascript"></script>
    <script src="~/Content/assets/js/app.js" type="text/javascript"></script>
    <script src="~/Content/assets/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Content/assets/js/jquery.dataTables.min.js"></script>

    <script src="~/Content/JavaS/salidasFucntion.js"></script>
    <script src="~/Content/JavaS/OrdenarTabla.js"></script>
    <script src="~/Content/JavaS/categoriaFuntions.js"></script>
    <script src="~/Content/JavaS/SuministrosFuntions.js"></script>
    <script src="~/Content/JavaS/RedirectionsPages.js"></script>
    <script src="~/Content/JavaS/ProductOrderFuntions.js"></script>
    <script src="~/Content/JavaS/EntradaFuctions.js"></script>
    <script src="~/Content/JavaS/DetallePOFuntions.js"></script>
    <script src="~/Content/JavaS/Login.js"></script>
    <script src="~/Content/JavaS/User.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            //initialize the javascript
            App.init();
            App.dataTables();
            App.dashboard();
        });

    </script>
</body>
</html>
