@model SuministrosProject.Models.Usuario

@{
    ViewBag.Title = "Delete";
    Layout = null;
}

<div class="" style="text-align:center;">
    <h2>DELETE</h2>
    <h3>Are you sure you want to delete this?</h3>
    <br />
    <hr />
</div>

<div class="row">
    <dl class="col-md-6">
        <dt>
            <br />
            ID
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdGafete)
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            PERFIL
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdPerfilNavigation.PerfilName)
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            NAME
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            LAST NAME
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Apellido)
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            USER
        </dt>

        <dd>
            @Html.DisplayFor(model => model.usuario)
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            STATE
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-group modal-footer">
            <br />
            <button type="button" data-dismiss="modal" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
            <button type="submit" value="save" class="btn btn-danger">Delete</button>
        </div>
    }
</div>
