@model SuministrosProject.Models.Suministro

@{
    ViewBag.Title = "Delete";
    Layout = null;
}

@{ 
    var idSuministro = Model.IdSuministro;
}
<div class="" style="text-align:center;">
    <h3>Are you sure you want to delete this supply?</h3>
</div>

<div>
    <hr />
    <br />
    <dl class="dl-horizontal">

        <dt>
            SERIE
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Serie)
            <br />
            <br />
        </dd>

        <dt>
            NUMBER PART
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdNumeroParteNavigation.Descripcion)
            <br />
            <br />
        </dd>

        <dt>
            LOCATION
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdLocalizacionNavigation.descripcion)
            <br />
            <br />
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="modal-footer">
            <button type="button" onclick="postDeleteSuministro(@idSuministro)" class="btn btn-primary" data-dismiss="modal">Delete</button>
            <button type="button" data-dismiss="modal" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
        </div>
    }
</div>

<script src="~/Content/JavaS/SuministrosFuntions.js"></script>
