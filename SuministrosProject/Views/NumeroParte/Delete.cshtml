@model SuministrosProject.Models.NumeroParte

@{
    ViewBag.Title = "Delete";
    Layout = null;   
}

<div class="" style="text-align:center;">
    <h2>DELETE</h2>
    <h3>Are you sure you want to delete this?</h3>
</div>

<div class="row">
    <hr />
    <dl class="col-md-6">
        <dt>
            <br />
            TYPE SUPPLY
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IdCategoriaNavigation.CategoriaDescripcion)
            <br />
            <br />
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            <br />
            NUMBER PART
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Descripcion)
            <br />
            <br />
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            MODEL
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Modelo)
            <br />
            <br />
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            BRAND
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Marca)
            <br />
            <br />
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            OBSERVATION
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Observacion)
            <br />
            <br />
        </dd>
    </dl>

    <dl class="col-md-6">
        <dt>
            STATE
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-group modal-footer">
            <br />
            @{ 
                int idPartumber = Convert.ToInt32(Model.IdNumeroParte);
            }

            <button type="button" data-dismiss="modal" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
            <button type="button" onclick="postDeleteNumeroParte(@idPartumber)" data-dismiss="modal" class="btn btn-danger">Delete</button>
        </div>

                }
</div>
<script src="~/Content/JavaS/NumeroParteFuntions.js"></script>
